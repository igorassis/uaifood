{
  "name": "atomic-koala-boilerplate",
  "version": "1.1.0",
  "description": "React boilerplate",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-serve --open",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config ./webpack.config.js --mode development --hot --inline",
    "build": "cross-env NODE_ENV=production webpack",
    "format": "prettier --write 'packages/**/*.js'",
    "test": "jest --watchAll --coverage",
    "test-watch": "jest --watch --no-coverage",
    "precommit": "lint-staged",
    "test-e2e": "cypress run --config fileServerFolder=e2e"
  },
  "lint-staged": {
    "*.{js,jsx,css}": [
      "prettier --write --single-quote",
      "eslint --fix --quiet",
      "git add"
    ]
  },
  "repository": "",
  "author": "Igor Pires",
  "license": "ISC",
  "private": false,
  "engines": {
    "node": ">=8",
    "npm": ">=3"
  },
  "dependencies": {
